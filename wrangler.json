{
  "name": "yazganxyz-apex-ui",
  "compatibility_date": "2025-05-05",
  "main": "./server.cloudflare.ts",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "./build/client"
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "routes": [
    {
      "pattern": "yazgan.xyz",
      "custom_domain": true
    },
    {
      "pattern": "www.yazgan.xyz",
      "custom_domain": true
    },
    {
      "pattern": "alperreha.yazgan.xyz",
      "custom_domain": true
    }
  ],
  "vars": { "GA_TRACKING_ID": "G-1234567890" }
}
